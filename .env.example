# Resend API Configuration
# Copy this file to .env and fill in your values

# Your Resend API key (get it from https://resend.com/api-keys)
RESEND_API_KEY=re_your_api_key_here

# Default sender email address (optional)
RESEND_DEFAULT_FROM=noreply@yourdomain.com

# Output format (optional): json, pretty, or table
RESEND_OUTPUT_FORMAT=pretty

# Log level (optional): silent, error, warn, info, debug
RESEND_LOG_LEVEL=info

# API base URL (optional - only change for testing)
# RESEND_API_BASE_URL=https://api.resend.com
