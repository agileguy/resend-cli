{
  "name": "resend-cli",
  "version": "0.1.0",
  "description": "Official CLI for Resend - The Email API for Developers",
  "type": "module",
  "bin": {
    "resend": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun --format esm && chmod +x dist/index.js",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "resend",
    "email",
    "cli",
    "api",
    "developer-tools"
  ],
  "author": "Resend",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/resendlabs/resend-cli"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.2.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.3"
  }
}
